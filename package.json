{
  "name": "bitsights-backend",
  "version": "0.0.0",
  "private": true,
  "main": "dist/src/index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "npm run lint && tsc",
    "clean": "npm run clean:dist && npm run clean:node",
    "clean:dist": "rm -rf dist",
    "clean:node": "rm -rf node_modules",
    "lint": "tslint -p .",
    "test": "npm run build && npm run test:unit && npm run test:integration",
    "test:integration": "npm run test:integration:one test/integration/features",
    "test:integration:one": "npx cucumber-js --exit --tags 'not @ignore' --require-module ts-node/register -r dist/test/integration/step-definitions/index.js",
    "test:unit": "mocha -require ts-node/register './test/unit/**/*.ts'"
  },
  "devDependencies": {
    "@types/async": "3.2.3",
    "@types/cucumber": "6.0.1",
    "@types/debug": "4.1.5",
    "@types/express": "4.17.6",
    "@types/lodash": "4.14.150",
    "@types/mocha": "7.0.2",
    "@types/morgan": "1.9.0",
    "@types/node": "13.13.2",
    "@types/redis": "2.8.18",
    "@types/request-promise-native": "1.0.17",
    "@types/slug": "0.9.1",
    "@types/uuid": "7.0.2",
    "@types/yargs": "15.0.4",
    "mocha": "7.1.1",
    "pickled-cucumber": "2.0.14",
    "tslint": "5.20.1",
    "tslint-config-airbnb": "5.11.2",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "@types/cors": "2.8.6",
    "async": "3.2.0",
    "bcoin": "git+https://github.com/bcoin-org/bcoin.git#semver:~2.1.2",
    "bottleneck": "2.19.5",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "debug": "4.1.1",
    "dos-config": "3.0.2",
    "express": "4.17.1",
    "lodash": "4.17.15",
    "morgan": "1.10.0",
    "redis": "3.0.2",
    "request": "2.88.2",
    "request-promise-native": "1.0.8",
    "slug": "2.1.1",
    "uuid": "7.0.3",
    "yargs": "15.3.1"
  }
}
