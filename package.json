{
  "name": "bitsights-backend",
  "version": "0.0.0",
  "private": true,
  "main": "dist/src/index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "npm run lint && tsc",
    "clean": "npm run clean:dist && npm run clean:node",
    "clean:dist": "rm -rf dist",
    "clean:node": "rm -rf node_modules",
    "lint": "tslint -p .",
    "test": "npm run build && npm run test:unit && npm run test:integration",
    "test:integration": "npm run test:integration:one test/integration/features",
    "test:integration:one": "npx cucumber-js --exit --tags 'not @ignore' --require-module ts-node/register -r dist/test/integration/step-definitions/index.js",
    "test:unit": "mocha -require ts-node/register './test/unit/**/*.ts'"
  },
  "devDependencies": {
    "@types/cucumber": "4.0.5",
    "@types/debug": "4.1.5",
    "@types/express": "4.17.2",
    "@types/lodash": "4.14.149",
    "@types/mocha": "5.2.7",
    "@types/morgan": "^1.7.37",
    "@types/node": "12.12.14",
    "@types/pug": "2.0.4",
    "@types/request-promise-native": "1.0.17",
    "@types/uuid": "3.4.6",
    "@types/yargs": "13.0.3",
    "mocha": "6.1.4",
    "pickled-cucumber": "2.0.2",
    "tslint": "5.20.1",
    "tslint-config-airbnb": "5.11.2",
    "typescript": "3.7.2"
  },
  "dependencies": {
    "@types/cors": "2.8.6",
    "bclient": "0.1.7",
    "cookie-parser": "~1.4.4",
    "cors": "2.8.5",
    "debug": "4.1.1",
    "dos-config": "3.0.2",
    "express": "4.17.1",
    "lodash": "4.17.15",
    "morgan": "~1.9.1",
    "pug": "2.0.4",
    "request": "2.88.0",
    "request-promise-native": "1.0.8",
    "uuid": "3.3.3",
    "yargs": "15.0.2"
  }
}
